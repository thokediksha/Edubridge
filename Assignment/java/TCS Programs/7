import java.util.*;
public class TwoNumbers 
{
public static void main(String[] args) 
{
Scanner sc=new Scanner(System.in);
int i,j,x,y;
System.out.println("Enter the no x & y");
x=sc.nextInt();
y=sc.nextInt();

for(i=1;i<=100;i++)
{
for(j=i;j<=100;j++)
{
if((i+j==x) && (i*j==y))
{
System.out.println("Two numbers are " +i+" "+j);
break;
}
}
}
}
}
